/* custom.css */

/* Change the background color of the site */
body {
  background-color: #e1e7ed;
}

/* Change the font of the site title */
.site-title {
  font-family: 'Helvetica', sans-serif;
  color: #333333;
}

/* Style the navigation bar */
.navbar {
  background-color: #333333;
}

.navbar a {
  color: #ffffff;
}

/* Customize links */
a {
  color: #0066cc;
}

a:hover {
  color: #ff6600;
}
