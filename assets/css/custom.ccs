/* custom.css */

/* Change the background color of the site */
body {
  background-color: #f0f0f0;
}

/* Change the font of the site title */
.site-title {
  font-family: 'Arial', sans-serif;
  color: #333333;
}

/* Style the navigation bar */
.navbar {
  background-color: #333333;
}

.navbar a {
  color: #ffffff;
}

/* Customize links */
a {
  color: #0066cc;
}

a:hover {
  color: #ff6600;
}
